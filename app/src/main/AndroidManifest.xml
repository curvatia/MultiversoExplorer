<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.multiversoexplorer">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/multiverso_explorer_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/multiverso_explorer_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.MultiversoExplorer">
        <activity
            android:name=".HomeWebViewActivity"
            android:exported="false" />
        <activity
            android:name=".ui.dashboard.GuardarBilletesActivity"
            android:exported="false" />
        <activity
            android:name=".ui.dashboard.FormularioViajes"
            android:exported="false" />
        <activity
            android:name=".ui.dashboard.WebviewActivity"
            android:exported="false" />
        <activity
            android:name=".data.model.AuthActivity"
            android:exported="false" />
        <activity
            android:name=".data.model.RegistroActivity"
            android:exported="false" />
        <activity
            android:name=".ui.login.LoginActivity"
            android:exported="false"
            android:label="@string/title_activity_login" />
        <activity
            android:name=".SplashActivity"
            android:exported="true" />
        <activity
            android:name=".ui.home.HomeViajesActivity"
            android:exported="true" />
        <activity
            android:name=".model.login.LoginActivity"
            android:exported="true" />
        <activity
            android:name=".BottomNavActivity"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:parentActivityName=".BottomNavActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>